     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <link rel="stylesheet" href="/stylesheets/bootstrap.css">
       <link href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.1/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.1/dist/js/bootstrap.bundle.min.js"></script>
     <link rel="stylesheet" href="/stylesheets/dashboard.css">
     <link rel="stylesheet" href="/stylesheets/profile_info.css">
     <script defer src="/javascripts/dashboard.js"></script>
   </head>
<body>
  <div class="sidebar">
    <div class="logo-details">
      <i class="bx bxs-book-reader icon"></i>
        <div class="logo_name">Online Article</div>
        <i class="bx bx-menu" id="btn" ></i>
    </div>
    <ul class="nav-list">
      <li>
          <i class="bx bx-search" ></i>
         <input type="text" placeholder="Search...">
         <span class="tooltip">Search</span>
      </li>
      <li>
        <a id="home" href="/api/dashboard">
          <i class="bx bx-grid-alt"></i>
          <span class="links_name">Dashboard</span>
        </a>
         <span class="tooltip">Dashboard</span>
      </li>
      <li>
       <a id="bloggers" href="/api/dashboard/users">
         <i class="bx bx-user" ></i>
         <span class="links_name">Bloggers</span>
       </a>
       <span class="tooltip">Bloggers</span>
     </li>
     <li>
       <a id="articles" href="#">
         <i class="bx bx-chat" ></i>
         <span class="links_name">Articles</span>
       </a>
       <span class="tooltip">Articles</span>

       <!-- submenu -->
          <div id="article-submenu" class="">
            <ul>
              <li>
                <a id="my-article" href="/api/article?id=<%= user._id %>">
                  <i class="bx bx-chat" ></i>
                  <span class="links_name">My Articles</span>
                </a>
              </li>
              <li>
                <a id="new-article" href="/api/article/new">
                  <i class="bx bx-chat" ></i>
                  <span class="links_name">New Article</span>
                </a>
              </li>
              <li>
                <a id="all-articles" href="/api/article">
                  <i class="bx bx-chat" ></i>
                  <span class="links_name">All Articles</span>
                </a>
              </li>
              
            </ul>
          </div>
       <!-- /submenu -->
     </li>
     <!-- <li>
       <a id="statistics" href="#">
         <i class="bx bx-pie-chart-alt-2" ></i>
         <span class="links_name">Statistics</span>
       </a>
       <span class="tooltip">Statistics</span>
     </li> -->
   
     <li>
       <a id="profile" href="/api/dashboard/profile">
         <i class="bx bx-heart" ></i>
         <span class="links_name">Profile info</span>
       </a>
       <span class="tooltip">Profile info</span>
     </li>
     <li>
       <a href="/api/dashboard/logout">
       
        <i class="bx bx-exit" ></i>
         <span class="links_name">Log Out</span>
       </a>
       <span class="tooltip">Log Out</span>
     </li>
     <li class="profile">
         <div class="profile-details">
           <img src="/images/avatar/profile.jpg" alt="profileImg">
           <div class="name_job">
             <div class="name">Reza Ahmad Nezhad</div>
             <div class="job">Backend Developer</div>
           </div>
         </div>
         <i class="bx bx-log-out" id="log_out" ></i>
     </li>
    </ul>
  </div>
